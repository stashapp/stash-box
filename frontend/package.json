  "scripts": {
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "lint:css": "stylelint 'src/**/*.scss'",
    "lint:js": "eslint_d --cache src/**/*.{ts,tsx}",
    "lint": "yarn lint:css && yarn lint:js",
    "start": "yarn run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "yarn run build && node -r dotenv/config express.js",
    "generate": "apollo client:codegen --target typescript --outputFlat src/definitions --no-addTypename"
  },
