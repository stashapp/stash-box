{
  "name": "stash-box-frontend",
  "version": "0.1.0",
  "description": "Stash-box",
  "license": "MIT",
  "scripts": {
    "start": "vite --host",
    "build": "vite build",
    "validate": "yarn lint && yarn format-check && tsc --noEmit",
    "lint": "yarn lint:css && yarn lint:js",
    "lint:js": "eslint --cache src/**/*.{ts,tsx}",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "generate": "apollo client:codegen --target typescript --outputFlat src/graphql/definitions",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,scss}\"",
    "format-check": "prettier --check \"src/**/!(generated-graphql).{js,jsx,ts,tsx,scss}\"",
    "analyze": "analyze=true vite build"
  },
  "engines": {
    "node": ">=12",
    "yarn": ">=1.15.2"
  },
  "resolutions": {
    "**/graphql": "^15.7.2"
  },
  "devDependencies": {
    "@rollup/plugin-graphql": "^1.0.0",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/lodash-es": "^4.17.5",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-react": "1.0.5",
    "apollo": "^2.33.9",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "2.4.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "sass": "~1.43.4",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-config-standard-scss": "^2.0.1",
    "stylelint-scss": "^4.0.0",
    "typescript": "~4.4.4",
    "vite": "^2.6.13",
    "vite-tsconfig-paths": "^3.3.17"
  },
  "dependencies": {
    "@apollo/client": "^3.4.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@hookform/resolvers": "2.8.2",
    "@types/react-helmet": "^6.1.4",
    "apollo-upload-client": "^16.0.0",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "date-fns": "^2.25.0",
    "graphql": "^15.7.2",
    "graphql-tag": "^2.12.5",
    "i18n-iso-countries": "^6.8.0",
    "lodash-es": "^4.17.21",
    "p-debounce": "^4.0.0",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "7.18.0",
    "react-markdown": "^7.1.0",
    "react-responsive-carousel": "^3.2.20",
    "react-router-dom": "^5.2.0",
    "react-select": "^5.2.1",
    "remark-breaks": "^3.0.1",
    "remark-external-links": "^9.0.1",
    "remark-gfm": "^3.0.0",
    "yup": "0.32.11"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
