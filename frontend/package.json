{
  "name": "stash-box-frontend",
  "version": "0.1.0",
  "description": "Stash-box",
  "license": "MIT",
  "scripts": {
    "start": "vite --host",
    "build": "vite build",
    "validate": "yarn lint && yarn format-check && tsc --noEmit",
    "lint": "yarn lint:css && yarn lint:js",
    "lint:js": "eslint --cache src/**/*.{ts,tsx}",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "generate": "graphql-codegen --config codegen.yml",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,scss,gql}\"",
    "format-check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,scss,gql}\"",
    "analyze": "analyze=true vite build"
  },
  "engines": {
    "node": ">=16",
    "yarn": ">=1.15.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/typed-document-node": "^3.0.0",
    "@graphql-codegen/typescript": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@rollup/plugin-graphql": "^2.0.2",
    "@types/apollo-upload-client": "^17.0.2",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globule": "^1.3.4",
    "prettier": "2.7.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "sass": "~1.56.1",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "~4.9.3",
    "vite": "^4.1.4",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@hookform/resolvers": "2.9.7",
    "apollo-upload-client": "^17.0.0",
    "bootstrap": "^5.2.2",
    "classnames": "^2.3.1",
    "date-fns": "^2.29.2",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "i18n-iso-countries": "^7.5.0",
    "lodash-es": "^4.17.21",
    "p-debounce": "^4.0.0",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.6.0",
    "react-bootstrap-typeahead": "^6.0.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "7.32.2",
    "react-markdown": "^8.0.3",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.6.1",
    "react-select": "^5.6.1",
    "rehype-external-links": "1.0.1",
    "remark-breaks": "^3.0.1",
    "remark-gfm": "^3.0.1",
    "yup": "0.32.11"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
